* TEST THE 'QSORT' FUNCTION
EQU MAX TO 1000
STIME = TIME()
RECORD = '' ; ERR = 0
FOR I = MAX TO 1 STEP -1
   RECORD<1, -1> = I
NEXT I
FOR I = 1 TO 10 ; CRT RECORD<1,I> ; NEXT I
CALL QSORT('AR', RECORD, ERR)
ETIME = TIME() - STIME
CRT 'ERR = ':ERR:' - TIME = ':ETIME:' SECONDS!'
FOR I = 1 TO 10 ; CRT RECORD<1,I> ; NEXT I
STOP
